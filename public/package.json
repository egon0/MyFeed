    "pouchdb": "*",
    "request": "~2.45.0",
    "rss": "~1.0.0",
    "rssparser": "*",
    "stylus": "~0.47.3",
    "truncate": "^1.0.4",
    "uglify-js": "~2.4.15",
    "urlify": "~0.3.5"
  },
