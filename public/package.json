{
  "name": "MyFeed",
  "version": "0.0.0",
  "description": "Just like a tumblr but more like facebook. if you know what i mean.",
  "main": "index.js",
  "scripts": {
    "start": "npm run concat & npm run browserify & node index.js",
	"concat": "find coffee/modules/ -type f -name '*.coffee' -exec cat {} \\; -exec echo \\; -exec echo \\; > coffee/_concat.coffee",
    "browserify": "watchify -t coffeeify coffee/main.coffee  -o _js/complete.js"
  },
  "keywords": [
    "facebook",
    "tumblr",
    "feed",
    "newsfeed",
    "timeline"
  ],
  "author": "Robert Agthe",
  "license": "ISC",
  "dependencies": {
    "angular": "*",
    "angular-sanitize": "*",
    "es6-shim": "^0.32.2",
    "jquery": "^2.1.4",
    "moment": "*",
    "remarkable": "*",
    "stackblur-agthe": "*",
    "twitter-widgets": "0.0.1",
    "bluebird": "~2.9.12",
    "body-parser": "~1.7.0",
    "bower-json": "~0.4.0",
    "cheerio": "^0.19.0",
    "coffee-middleware": "*",
    "coffee-script": "*",
    "dotenv": "~0.2.8",
    "express": "^4.1.1",
    "express-livereload": "0.0.24",
    "he": "^0.5.0",
    "jade": "^1.3.1",
    "jwt-simple": "~0.2.0",
    "md5": "~1.0.0",
    "myfeed-scrape": "*",
    "node-markdown": "~0.1.1",
    "passport": "~0.2.1",
