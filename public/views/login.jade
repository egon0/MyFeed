// extend layout1
extends includes/_layout

// extend block head
block head

	// Head - Time for meta tags
	+global_headstuff({title: user.username+" ~ MyFeed"})

// extend block body
block body
	if settings.bgimage
		- var bgstyle = "background-image: url("+settings.bgimage+");"

	.Background(style=bgstyle)
	+Navigation({active: 3})
	.ContentWrapper
		+AuthModule()
		+FollowerModule()
		+SettingsModule()